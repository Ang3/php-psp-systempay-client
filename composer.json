{
    "name": "ang3/php-psp-systempay-client",
    "type": "library",
    "description": "PHP Systempay PSP client",
    "keywords": ["php", "psp", "systempay", "client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joanis ROUANET",
            "homepage": "https://github.com/Ang3"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ang3\\Component\\PSP\\Systempay\\": "src/",
            "Ang3\\Component\\PSP\\Systempay\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "symfony/http-client": "^7.0",
        "symfony/property-access": "^7.0"
    },
    "require-dev": {
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^6.3"
    },
    "config": {
        "sort-packages": true
    }
}
